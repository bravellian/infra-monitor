{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PostmarkCache": {
    "TtlMinutes": 15
  },
  "SetupStorage": {
    "Mode": "Cookie"
  },
  "SqlSnapshots": {
    "Interval": "06:00:00",
    "RetentionDays": 14
  }
}
