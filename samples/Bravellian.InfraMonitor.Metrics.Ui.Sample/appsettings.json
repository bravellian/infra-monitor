{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MetricsUi": {
    "RefreshSeconds": 15,
    "Endpoints": [
      {
        "ServiceName": "SampleApp",
        "InstanceName": "Local",
        "EndpointUrl": "http://localhost:5000/metrics"
      }
    ],
    "PinnedMetrics": [
      "process.cpu.time",
      "http.server.request.duration_count"
    ]
  },
  "AllowedHosts": "*"
}
